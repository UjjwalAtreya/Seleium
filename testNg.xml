<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name=" Automation Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="Listners.WebDriverListner"></listener>
    </listeners>

    <!--LOGIN COMBINATION TEST-->
    <test name="LOGIN (Combinations) TEST" thread-count="2" verbose="2" parallel="methods">
        <parameter name="browserName" value="chrome"/>
        <classes>
            <class name="Tests.LoginTest">
                <methods>
                    <include name="testWithValidCredentials"/>
                    <include name="testWithValidCredentials1"/>
                    <include name="testWithValidCredentials2"/>

                </methods>
            </class>
        </classes>
    </test>



</suite>
